@model IEnumerable<app_remesas_parcial.Models.transacciones>

<a class="btn btn-success mb-3" asp-controller="Registro" asp-action="Index">CUENTA NUEVA A REGISTRAR:</a>
<br>
<div class="row justify-content-center">
    <table class="table table-striped table-bordered table-haver">
        <thead>
            <td>Remitente</td>
            <td>Destino</td>
            <td>Pais de Origen</td>
            <td>Pais de Destino</td>
            <td>Monto Enviado</td>
            <td>Tipo de Moneda</td>
            <td>Cambio</td>
            <td>Monto Total </td>
            <td>Estado</td>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Remitente)</td>
                    <td>@Html.DisplayFor(modelItem => item.Destino)</td>
                    <td>@Html.DisplayFor(modelItem => item.PaisOrigen)</td>
                    <td>@Html.DisplayFor(modelItem => item.PaisDestino)</td>
                    <td>@Html.DisplayFor(modelItem => item.MontoEnviado)</td>
                    <td>@Html.DisplayFor(modelItem => item.TipoMoneda)</td>
                    <td>@Html.DisplayFor(modelItem => item.Cambio)</td>
                    <td>@Html.DisplayFor(modelItem => item.MontoFinal)</td>
                    <td>@Html.DisplayFor(modelItem => item.Status)</td>
                </tr>
            }
        </tbody>
    </table>
</div>